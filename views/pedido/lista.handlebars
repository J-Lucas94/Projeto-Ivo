<div class="table1 col-12">
    <div class="table-responsive">
        <table class="table table-responsive">
            <thead>
                <tr>
                    <th></th>
                    <th>Produto</th>
                    <th>GBU</th>
                    <th>Valor Unit</th>
                    <th>Desc1 </th>
